/*program to print elements present in the odd positions of an array*/
import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter no.of elements to be stored in the array:");
		int n=sc.nextInt();
		int a[]=new int[n];
		for(int i=0;i<n;i++)
		{
		    System.out.println("Enter value at index-"+i+":");
		    a[i]=sc.nextInt();
		    
		}
		System.out.println("The array elements that are present in even positions are:");
		for(int i=0;i<n;i=i+2)
		{
		    System.out.println(a[i]);
		}
	}
}
