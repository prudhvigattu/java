package placement;
import java.util.*;
public class Perfect_Square {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter number:");
		int n=sc.nextInt();
		int temp=-1;
		for(int i=1;i<=n/2;i++)
		{
			if((i*i)==n)
				temp=1;
		}
		if(temp==1)
			System.out.println("Perfect square");
		else
			System.out.println("Not a Perfect square");

	}

}
