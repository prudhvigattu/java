package placement;
import java.util.Scanner;
public class N_Prime_Numbers {
	public static void main(String args[])
	{
		Scanner sc=new Scanner(System.in);
		int count=0;
		System.out.println("Enter range of a number:");
		int n=sc.nextInt();
		int a[]=new int[n];
		int i,j,z=0;
		for(j=2;j<=n;j++)
		{
			count=0;
		for(i=1;i<=j;i++) {
			if(j%i==0)
			{
				count++;
			}
		}
		if(count==2)
		{
			System.out.println(j);
		}
		}
		
				
	}

}
