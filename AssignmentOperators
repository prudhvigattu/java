package placementpreparation;

public class AssignmentOperators {

	public static void main(String[] args) {
		int a=10;
		a+=2;
		System.out.println(a);//12
		a-=4;
		System.out.println(a);//8
		a*=2;
		System.out.println(a);//16
		a/=2;
		System.out.println(a);//8
		a%=2;
		System.out.println(a);//0
		
	}

}
