package placementpreparation;
class Hod
{
	void info()
	{
		System.out.println("This is Hod class info method");
	}
}
class  Teacher extends Hod
{
	void info()
	{
		super.info();
		System.out.println("This is Teacher class info method");
	}
}
public class Super_Method {
	public static void main(String args[]) {
		Teacher t=new Teacher();
		t.info();
		
	}

}
